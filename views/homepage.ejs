	    	<div class="front cheesy-sentance">
	    		<span class="front cheesy"><h1>Create a Discussion Forum in Seconds</h1></span>
	    	</div>

	    	<div class="second-main-wrapper">

		    	<div class="front form-wrapper">

					<div class="creator-form reg" ng-show="isLoggedIn()">

					    <form class="homepage-forum reg" name="buildForumWU">
					  		
					  		<div class="input name" ng-class="{error: buildForumWU.comname.$invalid}">
									<label class="name">Community Name</label>
									<div class="faketb">
										<i class="fa fa-comment fa-lg"></i>
										<input class="text name" type="text"  placeholder="Community Name" data-wc-unique="{property: 'forumfirst'}" ng-change="changeURL(comname)" name="comname" ng-model="comname" ng-pattern="/^[a-zA-Z][a-zA-Z0-9_ -]{2,25}$/" required> </input>
									</div>
									<div class="invalid" ng-show="buildForumWU.comname.$dirty && buildForumWU.comname.$invalid"><br/>
									    <span class="errorMessage" ng-show="buildForumWU.comname.$error.pattern">Error: Invalid Name<br/></span>
									</div>
							</div>

							<div class="input url">
								<div class="form-group two" ng-class="{ 'has-error' : signup.comurl.$invalid && !signup.comurl.$pristine }">

								<label class="url">discussone.com/</label>
							  		<input class="text url" type="text" ng-model="comurl" name="comurl" ng-change="updateURL(comurl)" data-wc-unique="{property: 'forum'}" ng-pattern="/^[a-zA-Z][a-zA-Z0-9-]{2,25}$/" required> </input>
							  		<span class="errorMessage" ng-show="buildForumWU.comurl.$dirty && buildForumWU.comurl.$error.unique">URL already in use</span>
							  		<span class="errorMessage" ng-show="buildForumWU.comname.$dirty && buildForumWU.comname.$error.unique">URL already in use</span>
							  		<span class="errorMessage" ng-show="buildForumWU.comurl.$error.pattern">Error: Invalid link<br/></span>
						  		</div>
					  		</div>
					  		<a ng-click="options()" href="#" class="options"><span>Options</span></a>
							<div class="input access">
						  		<label class="access">Access Code <span>(Optional)</span></label>
						  		<div class="faketb">
						  			<i class="fa fa-info-circle"></i>
						  			<input class="text access" type="text" placeholder="" ng-model="accesscodebox"> </input>
						  		</div>
						  	</div>
					  		<div class="input reg">
						  		<label class="reg">Allow Registered Users</label>
						  		<input type="checkbox" ng-model="reguserbool" ng-true-value="true" ng-false-value="false" >
					  		</div>
					  		<div class="input anon">
						  		<label class="anon">Allow Anonymous Users</label>
						  		<input type="checkbox" ng-model="anonuserbool" ng-true-value="true" ng-false-value="false" >
					  		</div>
					  		<div class="submit-button">
					  			<button class="submit" ng-click="submitForm()" ng-disabled="buildForumWU.$invalid" type="submit">Create Forum Now!</button>
					  		</div>
					  	</form>
					</div>

					<div class="creator-form anon" ng-show="!isLoggedIn()">

					    <form class="homepage-forum anon" name="buildForum">
					  		
					  		<div class="input name" ng-class="{error: buildForum.comname.$invalid}">
									<label class="name">Community Name</label>
									<div class="faketb">
										<i class="fa fa-comment fa-lg"></i>
										<input class="text name" type="text"  placeholder="Community Name" data-wc-unique="{property: 'forumfirst'}" ng-change="changeURL(comname)" name="comname" ng-model="comname" ng-pattern="/^[a-zA-Z][a-zA-Z0-9_ -]{2,25}$/" required> </input>
									</div>
									<div class="invalid" ng-show="buildForum.comname.$dirty && buildForum.comname.$invalid"><br/>
									    <span class="errorMessage" ng-show="buildForum.comname.$error.pattern">Error: Invalid Name<br/></span>
									</div>
							</div>

							<div class="input url">
								
								<div class="form-group" ng-class="{ 'has-error' : signup.comurl.$invalid && !signup.comurl.$pristine }">
							  		<label class="url">discussone.com/</label>
							  		<input class="text url" type="text" ng-model="comurl" name="comurl" ng-change="updateURL(comurl)" data-wc-unique="{property: 'forum'}" ng-pattern="/^[a-zA-Z][a-zA-Z0-9-]{2,25}$/" required> </input>
							  		<span class="errorMessage" ng-show="buildForum.comurl.$dirty && buildForum.comurl.$error.unique">URL already in use</span>
							  		<span class="errorMessage" ng-show="buildForum.comname.$dirty && buildForum.comname.$error.unique">URL already in use</span>
							  		<span class="errorMessage" ng-show="buildForum.comurl.$error.pattern">Error: Invalid link<br/></span>
						  		</div>
					  		</div>

					  		<div class="input email">
								<label class="email">Email</label>
								<div class="form-group" ng-class="{ 'has-error' : signup.email.$invalid && !signup.email.$pristine }">
									<div class="faketb">
										<i class="fa fa-envelope fa-lg"></i>
										<input class="text url" type="email" placeholder="Email" ng-model="email" data-wc-unique="{property: 'email'}" style="width:200px;" name="email" ng-pattern="/^[_a-z0-9]+(\.[_a-z0-9]+)*@[a-z0-9-]+(\.[a-z0-9-]+)*(\.[a-z]{2,4})$/" required>
									</div>
									<span class="errorMessage" ng-show="buildForum.email.$dirty && buildForum.email.$error.unique">Email already in use</span>
									<span class="errorMessage" ng-show="buildForum.email.$error.pattern">Invalid Email<br/></span>
								</div>
							</div>
							<div class="input username">
								<label class="username">Username</label>
								<div class="control-group" ng-class="{ 'has-error' : signup.username.$invalid && !signup.username.$pristine }">
									<div class="faketb">
										<i class="fa fa-user fa-lg"></i>
										<input class="text username" type="text" placeholder="Username" ng-model="username"  data-wc-unique="{property: 'username'}" style="width:200px;" name="username" ng-minlength="4" ng-maxlength="22" required>
									</div>
									<span class="errorMessage" ng-show="buildForum.username.$dirty && buildForum.username.$error.unique">Username already in use</span>

							    </div>
							</div>

							<div class="input password">
								<label class="password">Password</label>
								<div class="control-group">
								<div class="faketb">
									<i class="fa fa-lock fa-lg"></i>
									<input class="text password" type="password" placeholder="Password" ng-model="password" style="width:200px;" name="password" ng-minlength="6" ng-maxlength="30" ng-pattern="/^[a-zA-Z0-9!@#$%^*]{6,16}$/"  required>
								</div>
								<span class="errorMessage" ng-show="buildForum.password.$dirty && buildForum.password.$error.pattern">Password Invalid</span>
								 </div>
							</div>
							<a ng-click="options()" href="#" class="options"><span>Options</span></a>
							<div class="input access">
						  		<label class="access">Access Code <span>(Optional)</span></label>
						  		<div class="faketb">
						  			<i class="fa fa-info-circle"></i>
						  			<input class="text access" type="text" placeholder="" ng-model="accesscodebox"> </input>
						  		</div>
						  	</div>
					  		<div class="input reg">
						  		<label class="reg">Allow Registered Users</label>
						  		<input type="checkbox" ng-model="reguserbool" ng-true-value="true" ng-false-value="false" >
					  		</div>
					  		<div class="input anon">
						  		<label class="anon">Allow Anonymous Users</label>
						  		<input type="checkbox" ng-model="anonuserbool" ng-true-value="true" ng-false-value="false" >
					  		</div>

					  		<div class="submit-button">
					  			<button class="submit" ng-click="submitFormWU()" ng-disabled="buildForum.$invalid" type="submit">Create Forum Now!</button>
					  		</div>
					  	</form>


					</div>

				</div> <!--second-main-wrapper-->


				<div class="front info-wrapper">
		    		<div class="laptop-i-cant-afford">
		    			<img src="/images/laptop2-837-500.png" class="img-laptop" alt="Forum Design Preview">
		    		</div>

		    		<div class="more-bs">
		    			<ul>
		    				<li><h2>Always Free</h2></li>
		    				<li><h2>Modern Design</h2></li>
		    				<li><h2>Easy to Use</h2></li>
		    				<li><h2>Make any Topic</h2></li>
		    			</ul>
		    		</div>
		    	</div>

			</div> <!--front-form-wrapper-->

		    	

		</div> <!--main-content-wrapper-->


		<div class="front first-footer">
			<div class="foot">
				<div class="f">
					<a href="/popular" target="_self">Popular Forums</a>
				</div>
			</div>
		</div>

		<div class="front last-footer">
			<div class="last-wrap">
				<ul>
					<li class="left"><a href="/" target="_self">About</a></li>
					<li class="left"><a href="/contact" target="_self">Contact</a></li>
					<li><a href="/" target="_self">Policy</a></li>
				</ul>
			</div>
		</div>
			
		</div> <!--main-front-wrapper-->

    	

		  	</script>

	
   
		<script type="text/ng-template" id="/homepage.html">

			<div class="main-front-wrapper">

		    	<nav class="front top-nav">
