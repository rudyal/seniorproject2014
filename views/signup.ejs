<!doctype html>
<html>
<head>
	<title>DiscussOne | Sign Up</title>
	<meta charset="utf-8">
	<meta name="keywords" content="discussone signup, discussone sign up, discuss one signup, discuss one sign-up">
  	<meta name="description" content="Sign-up for DiscussOne to start creating discussion forums instantly">
  	<link rel="icon" type="image/png" href="favicon-32x32.png" sizes="32x32" />
	<link rel="icon" type="image/png" href="favicon-16x16.png" sizes="16x16" />
	 <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="/stylesheets/style2.css">
    <link rel="stylesheet" href="/stylesheets/dist/style.css">
    <link href="/stylesheets/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="/stylesheets/froala_editor.min.css" rel="stylesheet" type="text/css" />
  	<link href="/stylesheets/froala_style.min.css" rel="stylesheet" type="text/css" />
  	<script type='text/javascript' src='/javascripts/jquery-1.11.0.js'></script>
    <script type='text/javascript' src="/javascripts/angular.min.js"></script>
    <script type='text/javascript' src="/javascripts/angular-ui-router.min.js"></script>
    <script type='text/javascript' src='/javascripts/infinite.js'></script>
    <script src="/javascripts/froala_editor.min.js"></script>
    <script src="/javascripts/froala-sanitize.js"></script>
    <script src="/javascripts/angular-froala.js"></script>
    <script type='text/javascript' src="/javascripts/angular-sanitize.js"></script>
    <script type='text/javascript' src="/javascripts/moment.min.js"></script>
    <script type='text/javascript' src="/javascripts/ga.js"></script>
	<script type='text/javascript' src="/javascripts/angular-momentjs/angular-momentjs.js"></script>
	<script type='text/javascript' src="/javascripts/angular-smooth-scroll.js"></script>
    <script src="/javascripts/angularApp.js"></script>
    <base href="/" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1"> 
</head>
<body ng-app="forumBody" class="white" ng-controller="ForumCtrl">
	<nav class="front top-nav">
	    	<div class="nav-content-wrapper">
		    	<div class="logo-wrapper">
	    			<a href="/"><img src="/images/discuss-one-logo-2.png" class="img-laptop" alt="DiscussOne Logo"></a>
	    		</div>
		    	<div class="account-wrapper">
		    		<div class="second-account">
		    			<div class="social">
			    				<a href="https://www.facebook.com/DiscussOne" target="_blank"><img src="/images/facebook-gray.png" class="img-laptop" alt="Facebook Icon"></a>
		    					<a href="https://twitter.com/discussone" target="_blank"><img src="/images/twitter-gray.png" class="img-laptop" alt="Twitter Icon"></a>
			    			</div>
			    		<ul>
				    		<li class="left" ng-show="!isLoggedIn()"><a href="/login" target="_self">Login</a></li>
							<li class="right" ng-show="!isLoggedIn()"><a href="/signup" target="_self">Sign-up</a></li>
				    	</ul>
			    	</div>
		    	</div>
		    	<div class="account-wrapper-mobile">
	    			<div><a ng-click="showMobile()" href="javascript:void(0)">Menu</a></div>	    			
			    </div>
		</div>
	</nav>
<div class="main-content-wrapper pro account">	
	<ul class="mobile-menu">
		<li class="left" ng-show="!isLoggedIn()"><a href="/login" target="_self"><div>Login</div></a></li>
		<li class="right" ng-show="!isLoggedIn()"><a href="/signup" target="_self"><div>Sign-up</div></a></li>
		<li class="left" ng-show="isLoggedIn()"><a href="/profile"  target="_self"><div>{{user.username}}</div></a></li>
		<li class="right" ng-show="isLoggedIn()"><a href="/logout"  target="_self"><div>logout</div></a></li>
	</ul>
<div class="container">



<div class="col-sm-6 col-sm-offset-3">

	<h1><span class="fa fa-sign-in"></span> Create Account</h1>

	<!-- show any messages that come back with authentication -->
	<% if (message.length > 0) { %>
		<div class="alert alert-danger"><%= message %></div>
	<% } %>

	<!-- LOGIN FORM -->
	<form name="signup2" action="/signup" method="post" enctype="multipart/form-data">
		<div class="form-group">
					<label>Email</label>
					<div class="form-group" ng-class="{ 'has-error' : signup2.email.$invalid && !signup2.email.$pristine }">
						<input type="email" class="form-control" ng-model="email" data-wc-unique="{property: 'email'}" style="width:200px;" name="email" ng-pattern="/^[_a-z0-9]+(\.[_a-z0-9]+)*@[a-z0-9-]+(\.[a-z0-9-]+)*(\.[a-z]{2,4})$/" required>
						<span class="errorMessage" ng-show="signup2.email.$dirty && signup2.email.$error.unique">Email already in use</span>
						<span class="errorMessage" ng-show="signup2.email.$error.pattern">Invalid Email<br/></span>
					</div>
				</div>
		<div class="form-group">
					<label>Username</label>
					<div class="control-group">
						<input type="text" class="form-control" ng-model="username" data-wc-unique="{property: 'username'}" style="width:200px;" name="username" ng-minlength="4" ng-maxlength="22" ng-pattern="/^[a-zA-Z0-9_-]{3,16}$/" required>
						<span class="errorMessage" ng-show="signup2.username.$dirty && signup2.username.$error.unique">Username already in use</span>
						<span class="errorMessage" ng-show="signup2.username.$dirty && signup2.username.$error.pattern">Invalid Username</span>

				    </div>
				</div>

		<div class="form-group">
			<label>Password</label>
			<div class="control-group">
			<input type="password" class="form-control" ng-model="password" name="password" ng-minlength="6" ng-maxlength="30" ng-pattern="/^[a-zA-Z0-9!@#$%^*]{6,16}$/"  required>
			<span class="errorMessage" ng-show="signup2.password.$dirty && signup2.password.$error.pattern">Password Invalid</span>
			 </div>
		</div>

		<div class="form-group">
			<label>Profile Image</label>
			<input type="file" name="file" class="form-control" >
		</div>

		

		<button type="submit" class="btn btn-warning btn-lg">Signup</button>
	</form>

	<hr>

	<p>Already have an account? <a href="/login">Login</a></p>
	<p>Or go <a href="/">home</a>.</p>

</div>
</div>
</div>

</div>
</body>
</html>
